{
  "name": "client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node server",
    "build": "yarn run build:server && yarn run build:client",
    "build:client": "rm -rf  dist/client && vite build --outDir dist/client",
    "build:server": "rm -rf  dist/server && vite build --outDir dist/server --ssr src/entry-server.tsx",
    "start": "NODE_ENV=production node server",
    "lint": "eslint .",
    "list:fix": "eslint --fix .",
    "format": "prettier --write .",
    "test": "jest ./ --maxWorkers 1"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.6",
    "@hookform/resolvers": "2.9.11",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.12.1",
    "@mui/x-data-grid": "6.3.0",
    "@reduxjs/toolkit": "1.9.5",
    "axios": "1.3.5",
    "dotenv": "16.0.2",
    "eslint-config-prettier": "8.5.0",
    "helmet": "7.0.0",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.9",
    "react-redux": "8.0.5",
    "react-router-dom": "6.10.0",
    "serialize-javascript": "6.0.0",
    "yup": "1.1.1"
  },
  "devDependencies": {
    "@hookform/devtools": "4.3.1",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "13.4.0",
    "@types/compression": "1.7.2",
    "@types/jest": "28.1.8",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "@vitejs/plugin-react": "2.0.1",
    "compression": "1.7.4",
    "eslint": "8.22.0",
    "jest": "28",
    "jest-environment-jsdom": "29.0.1",
    "lefthook": "1.1.1",
    "prettier": "2.7.1",
    "serve-static": "1.15.0",
    "ts-jest": "28.0.8",
    "typescript": "4.8.2",
    "vite": "3.0.7"
  },
